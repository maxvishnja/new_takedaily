{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.5.9",
		"laravel/framework": "5.2.*",
		"league/flysystem-aws-s3-v3": "^1.0",
		"aws/aws-sdk-php": "^3.12",
		"guzzlehttp/guzzle": "^6.1",
		"erusev/parsedown": "^1.6",
		"barryvdh/laravel-ide-helper": "^2.1",
		"barryvdh/laravel-debugbar": "^2.1",
		"intervention/image": "^2.3",
		"barryvdh/laravel-dompdf": "^0.8.0",
		"stripe/stripe-php": "^3.5",
		"jenssegers/date": "^3.1",
		"gilak/nolock": "^1.0",
		"mollie/laravel-mollie": "^1.1",
		"predis/predis": "^1.0",
		"watson/rememberable": "^1.0",
		"doctrine/dbal": "^2.5",
		"skovmand/mailchimp-laravel": "^1.0",
		"pda/Pheanstalk": "^3.1",
		"maatwebsite/excel": "^2.1.0",
		"vinkla/instagram": "^7.0",
		"php-http/message": "^1.6",
		"php-http/guzzle6-adapter": "^1.1",
		"milon/barcode": "^5.3",
		"mbarwick83/instagram": "^1.11",
		"facebook/php-ads-sdk": "2.11.*",
		"chumper/zipper": "1.0.x"

	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~4.0",
		"symfony/css-selector": "2.8.*|3.0.*",
		"symfony/dom-crawler": "2.8.*|3.0.*"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"Barryvdh\\TranslationManager\\": "app/barryvdh/laravel-translation-manager/src"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		],
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
